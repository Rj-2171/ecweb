<!DOCTYPE html>
<html>
    <body>
        <h3>Records</h3>
          <% records.forEach(function(row){ %>
          <p><%= row.name %></p>
            <p><%= row.email %></p>
            <p><%= row.age %></p>
            <p><%= row.dob%></p>
            <p><%= row.phone %></p>
          <% })}else{ %>
            <p>
                No Record Found
            </p>
            <% } %>
    </body>
</html>

        